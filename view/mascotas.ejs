<%- include("template/cabecera",{tituloWeb:'pagina de mascotas'})%>

<div class="container">
                <table class="table">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">nombre</th>
                    <th scope="col">description</th>
                    <th scope="col">accion</th>
                </tr>
                </thead>
                <tbody>
                    <% if (arrayMascotas.length > 0) { %>
                        <% arrayMascotas.forEach(mascota => { %>
                            <tr>
                                <th scope="row"><%= mascota.id %> </th>
                                <td><%= mascota.nombre %></td>
                                <td><%= mascota.description%></td>
                                <td>@mdo</td>
                            </tr>
                        <% }) %>
                    <% } %>
                </tbody>
            </table>
</div>